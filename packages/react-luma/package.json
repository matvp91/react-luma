{
  "name": "react-luma",
  "version": "0.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./navigation": {
      "import": "./dist/navigation/index.js",
      "default": "./dist/navigation/index.cjs"
    },
    "./animation": {
      "import": "./dist/animation/index.js",
      "default": "./dist/animation/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@react-luma/tsconfig": "workspace:*",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-reconciler": "0.28.0",
    "@types/stats.js": "^0.17.0",
    "@types/yoga-layout": "^1.9.4",
    "esbuild": "^0.16.7",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@pixi/canvas-renderer": "^7.0.5",
    "@react-pdf/yoga": "^3.0.0",
    "@tweenjs/tween.js": "^18.6.4",
    "pixi.js": "^7.0.5",
    "pixi.js-legacy": "^7.0.5",
    "react-reconciler": "0.29.0",
    "stats.js": "^0.17.0",
    "ts-custom-error": "^3.3.1",
    "typed-assert": "^1.0.9"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/navigation/index.ts",
      "src/animation/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  }
}
